<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="UniBuddy assignment questions">
    <groups>
        <run>
            <include name="Sanity"/>
        </run>
    </groups>
    <listeners>
    <listener class-name="reporting.ExtentReporter.Reporter"></listener>
</listeners>

    <test name="Unibuddy live test cases">
        <classes>
            <class name="com.unibuddy.qa.testCases.SearchService"></class>
            <class name="com.unibuddy.qa.testCases.AuthorService"></class>
        </classes>
    </test>
</suite>